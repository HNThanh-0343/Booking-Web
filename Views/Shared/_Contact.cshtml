@using WEBSITE_TRAVELBOOKING.Infrastructure;
@using WEBSITE_TRAVELBOOKING.Services
@{
    UnitOfWork _unitOfWorkCommon = new UnitOfWork(new WebsiteCmsBookingContext());
    var contacs = _unitOfWorkCommon.Repository<SysContact>().GetAll(filter: c => c.Status == true).FirstOrDefault();
    var changecl4 = "list-inline u-header__topbar-nav-divider mb-0";
    var changecl5 = "btn btn-sm btn-icon btn-pill btn-soft-white btn-bg-transparent transition-3d-hover";
    var tDetail = ViewData["Detail"];
    if (tDetail != null)
    {
        changecl4 = "list-inline list-inline-dark u-header__topbar-nav-divider--dark mb-0";
        changecl5 = "btn btn-xs btn-icon btn-pill btn-soft-dark btn-bg-transparent transition-3d-hover";
    }
}
<ul class="@changecl4">
    <li class="list-inline-item mr-0">
        <a href="tel:@contacs.Phone" class="u-header__navbar-link">@contacs.Phone</a>
    </li>
    <li class="list-inline-item mr-0">
        <a href="mailto:@contacs.Email" class="u-header__navbar-link">@contacs.Email</a>
    </li>
</ul>
<div class="ml-auto d-flex align-items-center">
    <ul class="list-inline mb-0 mr-2 pr-1">
        <li class="list-inline-item">
            <a class="@changecl5" href="@contacs.Urlfb" target="_blank" aria-label="Facebook">
                <span class="fab fa-facebook-f btn-icon__inner" aria-hidden="true"></span>
            </a>
        </li>
        <li class="list-inline-item">
            <a class="@changecl5" href="@contacs.Urltwi" target="_blank" aria-label="Twitter">
                <span class="fab fa-twitter btn-icon__inner" aria-hidden="true"></span>
            </a>
        </li>
        <li class="list-inline-item">
            <a class="@changecl5" href="@contacs.Urlinta" target="_blank" aria-label="Instagram">
                <span class="fab fa-instagram btn-icon__inner" aria-hidden="true"></span>
            </a>
        </li>
        <li class="list-inline-item">
            <a class="@changecl5" href="@contacs.Urlin" target="_blank" aria-label="LinkedIn">
                <span class="fab fa-linkedin-in btn-icon__inner" aria-hidden="true"></span>
            </a>
        </li>
    </ul>
