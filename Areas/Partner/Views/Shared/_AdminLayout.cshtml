<style>
    .mainslider h3 {
        background: #fff;
        color: #3498db;
        font-size: 36px;
        line-height: 100px;
        margin: 0px;
        padding: 2%;
        position: relative;
        text-align: center;
        /* height: 100%; */
    }

    .location-card {
        border: 1px solid #dee2e6;
        border-radius: 8px;
        padding: 17px;
        text-align: center;
        transition: all 0.3s ease-in-out;
    }

    .location-card-no-icon {
        border: 1px solid #dee2e6;
        border-radius: 8px;
        padding: 8px 5px;
        text-align: center;
        transition: all 0.3s ease-in-out;
    }

    .location-icon {
        width: 42px;
        height: 42px;
        background-color: #2e6b47;
        /* Màu xanh đậm */
        color: white;
        border-radius: 50%;
        display: inline-flex;
        align-items: center;
        justify-content: center;
        font-size: 20px;
        margin-bottom: 10px;
    }
</style>

